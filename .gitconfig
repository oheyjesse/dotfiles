[alias]
  # List all aliases:
  aliases = "!git config -l | grep alias | cut -c 7-"
  co = checkout
  br = branch
  ci = commit
  st = status
  bd = branch
  mg = merge
  aa = !git add -A && git status
  check = !git remote update && git status
  dad = !curl https://icanhazdadjoke.com/ && git add
  # Prettified Git Log
  ls = log --pretty=format:"%C(yellow)%h\\ %C(cyan)%ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
  # Pretty Git Log (With Timestamps)
  lst = log --pretty=format:"%C(yellow)%h\\ %C(cyan)%ai%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
  # Coloured Git Log with Line Changes
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
  # As above with no colour
  lnc = log --pretty=format:"%h\\ %s\\ [%cn]"
